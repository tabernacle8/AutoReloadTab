  {
    "name": "Auto Tab Reload",
    "version": "1.0",
    "description": "Reloads the tab.. thats it",
    "permissions": ["declarativeContent", "storage"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "page_action": {
      "default_popup": "popup.html"
    },
    "manifest_version": 2
  }