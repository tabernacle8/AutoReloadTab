{
  "name": "Simple Tab Reloader",
  "version": "1.1.5",
  "description": "Refresh your tabs, without all those annoying features",
  "permissions": ["declarativeContent", "storage"],
  "background": {
    "scripts": ["background.js", "reloadTabBackground.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}