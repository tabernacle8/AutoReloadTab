{
  "name": "Simple Tab Reloader",
  "version": "3.1.0",
  "description": "Refresh your tabs, without all those annoying features",
  "permissions": ["declarativeContent", "storage", "alarms"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": { "16": "Reload16.png",
    "48": "Reload48.png",
   "128": "Reload128.png" },
  "manifest_version": 3
}
